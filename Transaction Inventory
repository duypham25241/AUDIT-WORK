CREATE TABLE INV_RPT_044_v2_GD2 (
    DonVi NVARCHAR(MAX),
    SoChungTu NVARCHAR(MAX),
    NgayChungTu NVARCHAR(MAX),
    LoaiGiaoDich NVARCHAR(MAX),
    MaReasonCode NVARCHAR(MAX),
    TenReasonCode NVARCHAR(MAX),
    DienGiai NVARCHAR(MAX),
    Machuyen NVARCHAR(MAX),
    Tenchuyen NVARCHAR(MAX),
    MaVatTu NVARCHAR(MAX),
    TenVatTu NVARCHAR(MAX),
    SoLo NVARCHAR(MAX),
    LPN NVARCHAR(MAX),
    SoLoNL NVARCHAR(MAX),
    SoLuong1 NVARCHAR(MAX),
    SoLuong2 NVARCHAR(MAX),
    SoLuong3 NVARCHAR(MAX),
    SoMetVuong NVARCHAR(MAX),
    SoMetVuongVH NVARCHAR(MAX),
    DonGia NVARCHAR(MAX),
    SoTien NVARCHAR(MAX),
    MaKho NVARCHAR(MAX),
    TenKho NVARCHAR(MAX),
    MaSubinventory NVARCHAR(MAX),
    TenSubinventory NVARCHAR(MAX),
    Locator NVARCHAR(MAX),
    DVT1 NVARCHAR(MAX),
    DVT2 NVARCHAR(MAX),
    DVT3 NVARCHAR(MAX),
    TaiKhoanKho NVARCHAR(MAX),
    TaiKhoanChiPhi NVARCHAR(MAX),
    Lenhsx NVARCHAR(MAX),
    Trangthaisx NVARCHAR(MAX),
    Mataisan NVARCHAR(MAX),
    Mucdichsudung NVARCHAR(MAX),
    Ghichu NVARCHAR(MAX),
    TRANSACTION_ID NVARCHAR(MAX),
    SoCTgoc NVARCHAR(MAX),
    Makhonhan NVARCHAR(MAX),
    Shipmentnumber VARCHAR(MAX)
);

-- Q1
BULK INSERT INV_RPT_044_v2_GD2
FROM 'D:\Downloads\DATA\INV044\INV044 Q1.25.csv'
WITH (
    FIRSTROW=2,
    FIELDTERMINATOR=';',
    ROWTERMINATOR='\n',
    DATAFILETYPE='char',
    CODEPAGE = '65001',
    TABLOCK,
    MAXERRORS = 1000
);
